{
	"default_module": "powerline.segments.common",
	"segment_data": {
		"uptime": {
			"before": ""
		},
		"external_ip": {
			"before": ""
		},
		"date": {
			"before": ""
		}
	},

  
  
	"segments": {
		"right": [

      {
              "name": "network_load",
              "priority": 50
      },
      {
        "name": "external_ip"
      },
      {
        "name": "user"
      },
      

      {
          "name": "osx_battery",
          "module": "osx_powerline_battery.segments",
          "args": {
            "gamify": false,
            "steps": 20
          }
      },
      {
				"name": "date",
				"args": {
					"format": "%I:%M",
					"istime": true
				}
			},
			
      {
				"name": "hostname",
        "args": {
          "exclude_domain": true
        }
			
      }
		]
	}
}
