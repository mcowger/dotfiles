{
	"default_module": "powerline.segments.common",
	"segment_data": {
		"uptime": {
			"before": ""
		},
		"external_ip": {
			"before": ""
		},
		"date": {
			"before": ""
		}
	},

  
  
	"segments": {
		"right": [

      {
              "name": "network_load",
              "priority": 50,
              "args": {
                "sent_format": "⬆ {value:>8}",
                "recv_format": "⬇ {value:>8}",
                "suffix": "B/s "
              }
      },
      {
        "name": "external_ip"
      },
      {
        "name": "user"
      },
      

      {
				"name": "date",
				"args": {
					"format": "%I:%M",
					"istime": true
				}
			},
			
      {
				"name": "hostname",
        "args": {
          "exclude_domain": true
        }
			
      }
		]
	}
}
